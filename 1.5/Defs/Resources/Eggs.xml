    <defName>EggBrushTurkeyUnfertilized</defName>
    <label>brush turkey egg (unfert.)</label>
    <description>An unfertilized brush turkey egg. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <color>(228,185,135)</color>
    </graphicData>
    <statBases>
      <MarketValue>7.0</MarketValue>
    </statBases>
  </ThingDef>
