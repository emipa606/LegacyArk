    <defName>EggBrushTurkeyFertilized</defName>
    <label>brush turkey egg (fert.)</label>
    <description>A fertilized brush turkey egg. If all goes well, it should hatch into a chick. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(245,245,245)</color>
    </graphicData>
    <statBases>
      <MarketValue>7.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>3.5</hatcherDaystoHatch>
        <hatcherPawn>BrushTurkey</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
